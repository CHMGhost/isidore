echo '> create host foo'
create host foo
echo '> host foo set commissioned '1986-10-21''
host foo set commissioned '1986-10-21'
echo '> host foo set decommissioned '2015-10-21''
host foo set decommissioned '2015-10-21'
echo '> host foo set description 'Great Scott!''
host foo set description 'Great Scott!'
echo '> host foo show commissioned'
host foo show commissioned
echo '> host foo show decommissioned'
host foo show decommissioned
echo '> host foo show description'
host foo show description
echo '> host foo show all'
host foo show all
echo '> delete host foo'
delete host foo

